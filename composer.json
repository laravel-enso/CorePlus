{
    "name": "laravel-enso/coreplus",
    "description": "Laravel Enso Core Plus. Extra features on Users and Owners Table",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "mishu",
            "email": "mishu@xtelecom.ro"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.4.*",
        "laravel-enso/core": "1.0.*",
        "laravel-enso/cnpvalidator": "1.0.*",
        "laravel-enso/commentsmanager": "1.0.*",
        "laravel-enso/documentsmanager": "1.0.*",
        "laravel-enso/statisticsmanager": "1.0.*",
        "jenssegers/date": "^3.2",
        "barryvdh/laravel-debugbar": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\CorePlus\\": "src/"
        }
    },
    "scripts":{
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan optimize"
        ]
    }
}